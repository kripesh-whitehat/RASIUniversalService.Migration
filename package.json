{
  "name": "RasiUniversalServices",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --max_old_space_size=5120 app.js",
    "start-stage": "cross-env NODE_ENV=staging node app.js",
    "deploy-winservice": " cross-env NODE_ENV=production node --max_old_space_size=5120 app.js && cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "deploy-production": "rm -f ./log-production.log && pm2 kill && cross-env NODE_ENV=production pm2 start app.js -l ./log-production.log --name universal && cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "author": "RASI",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bcp": "^0.2.2",
    "bcrypt": "^5.0.1",
    "bull": "^3.22.7",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "memorystore": "^1.6.6",
    "mssql": "^7.1.3",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "tedious": "^11.0.9",
    "tedious-connection-pool": "^1.0.5"
  }
}
